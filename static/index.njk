<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title>picto</title>
  <link rel="stylesheet" href="/css/libs/awesomplete.css" />
  <link rel="stylesheet" href="/css/libs/perfect-scrollbar.min.css" />
  <link rel="stylesheet" href="/css/core.css" />
  <link rel="stylesheet" href="/css/search.css" />
  <script src="/js/libs/awesomplete.min.js"></script>
  <script src="/js/libs/perfect-scrollbar.min.js"></script>
  <script src="/js/xhr.js"></script>
  <script src="/js/nav.js"></script>
  <script src="/js/index.js"></script>
  <link rel="stylesheet" href="/css/libs/material-icons.css" />
  <link rel="stylesheet" href="/css/nav.css" />
  <link rel="stylesheet" href="/css/loadingbar.css" />
  <script src="/js/searce.js"></script>
  <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
</head>

<body>
  <div class="nav">
    <div class="top">
      {% for nav in navs.top %}
        <i class="material-icons" onclick="nav.changePath('/{{ nav.path }}',false,true)">{{ nav.icon }}</i>
      {% endfor %}
    </div>
    <div class="bottem">
      {% for nav in navs.bottom %}
        <i class="material-icons" onclick="nav.changePath('/{{ nav.path }}',false,true)">{{ nav.icon }}</i>
      {% endfor %}
    </div>
  </div>

  <div class="content">
    <input data-multiple />
    <br>
    <div class="tiles">

    </div>
  </div>
</body>

</html>
